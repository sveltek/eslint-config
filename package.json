{
  "name": "@sveltek/eslint-config",
  "version": "1.1.1",
  "author": "Sveltek",
  "contributors": [
    {
      "name": "Ivo Dolenc",
      "url": "https://github.com/ivodolenc"
    }
  ],
  "description": "Sveltek's config for ESLint.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sveltek/eslint-config.git"
  },
  "homepage": "https://github.com/sveltek/eslint-config",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "sveltek",
    "svelte",
    "sveltekit",
    "eslint",
    "eslint-config",
    "presets",
    "config"
  ],
  "scripts": {
    "build": "hyperbundler",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.34.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "svelte-eslint-parser": "^1.3.1"
  },
  "devDependencies": {
    "@hypernym/bundler": "^0.14.4",
    "@sveltek/prettier-config": "^1.0.4",
    "@sveltek/tsconfig": "^1.0.1",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
